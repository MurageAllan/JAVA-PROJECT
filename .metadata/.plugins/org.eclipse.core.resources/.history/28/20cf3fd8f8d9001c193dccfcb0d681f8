package recap1;

public abstract class Bird {
	private int age;
	private String name;
	private int ID;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	abstract void fly();

	abstract void feathers();

	abstract void beak();

	public void run() {
		fly();
		feathers();
		beak();
	}
	

}
