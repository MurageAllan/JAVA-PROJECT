package recap1;

public class Pigeon extends Bird {

	@Override
	void fly() {
		System.out.println("This pigeon is flying.");
		
	}

	@Override
	void feathers() {
		System.out.println("This pigeon has smooth feathers.");
		
	}

	@Override
	void beak() {
		System.out.println("The pigeon's beak is short.");
		
	}

}
